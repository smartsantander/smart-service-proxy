#------------
#Virtual IPv4 HTTP services for CoAP devices
#------------
ENABLE_VIRTUAL_HTTP_SERVER_OVER_IPv4 = true
IPv4_SERVER_DNS_WILDCARD_POSTFIX = spitfire.wuxi.cn

